li = [5,2,7,4,0,9,8,6] #Дан массив чисел
n = 1                  #Присваивание n = 1
while n < len(li):     #Пока n меньше чем длинна массива
     for i in range(len(li)-n): #Для i-го значения в диапазоне (длинна массива - n )
          if li[i] > li[i+1]:   # Если i-ое > i-го + 1
               li[i],li[i+1] = li[i+1],li[i]
     n += 1
print(li)

